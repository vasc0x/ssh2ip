{
    "name": "SSH to IP",
    "description": "Highlight an IP and right click to SSH to it",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
	"action": {
		"default_popup": "popup.html"
	},
    "permissions":[
        "contextMenus",
        "tabs",
		"storage"
    ],
    "icons": {
        "16": "icon.png"
    }
}